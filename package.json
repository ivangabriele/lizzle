{
  "name": "lichess-trainer",
  "description": "Train you chess puzzles.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": "9",
    "yarn": "3"
  },
  "scripts": {
    "build": "next build",
    "data:synchronize": "sh ./scripts/data/synchronize.sh",
    "db:generate": "yarn pnpify prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:generate",
    "dev": "next dev",
    "dev:docker": "docker compose -f ./config/docker-compose.yml down -v && docker compose -f ./config/docker-compose.yml up -d db",
    "dev:docker:stop": "docker compose -f ./config/docker-compose.yml down -v",
    "dev:migrate": "prisma migrate dev",
    "dev:setup": "sh ./scripts/build/postinstall.sh",
    "dev:setup:vscode": "yarn dlx @yarnpkg/sdks vscode",
    "start": "sh ./scripts/build/start.sh",
    "test": "yarn test:lint && yarn test:type",
    "test:lint": "eslint --ext cjs,js,jsx,ts,tsx .",
    "test:type": "tsc -p ./tsconfig.dev.json"
  },
  "dependencies": {
    "@ivangabriele/tsconfig-next": "1.2.2",
    "@prisma/client": "^4.9.0",
    "@swc/core": "1.2.197",
    "@types/chess.js": "0.13.2",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "17.0.42",
    "@types/ramda": "0.28.13",
    "@types/react": "18.0.14",
    "@types/styled-components": "5.1.15",
    "@yarnpkg/pnpify": "4.0.0-rc.11",
    "bhala": "3.0.4",
    "bootstrap": "5.1.3",
    "chess.js": "0.13.3",
    "chessground": "8.2.1",
    "esm-path": "1.0.1",
    "ky": "0.30.0",
    "ky-universal": "0.10.0",
    "lodash.debounce": "4.0.8",
    "next": "12.1.6",
    "numeral": "2.0.6",
    "prisma": "^4.9.0",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-bootstrap": "2.4.0",
    "react-dom": "17.0.2",
    "react-is": "18.2.0",
    "styled-components": "5.3.3",
    "type-fest": "2.13.1",
    "typescript": "4.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.2",
    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-typescript-next": "^6.0.1",
    "@ivangabriele/prettier-config": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "csvtojson": "2.0.10",
    "dotenv": "16.0.1",
    "eslint": "8.17.0",
    "husky": "8.0.0",
    "lint-staged": "13.0.2",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "*.{cjs,js,jsx,ts,tsx}": "eslint --ext cjs,js,jsx,ts,tsx"
  },
  "packageManager": "yarn@3.3.1",
  "prettier": "@ivangabriele/prettier-config"
}
