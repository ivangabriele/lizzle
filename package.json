{
  "name": "lizzle",
  "description": "Train your chess puzzles using Lichess database.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "19",
    "npm": "9"
  },
  "scripts": {
    "build": "sh ./scripts/build/build.sh",
    "data:sync": "sh ./scripts/data/synchronize.sh",
    "db:generate": "yarn pnpify prisma generate",
    "db:migrate": "prisma migrate deploy && yarn db:generate",
    "dev": "next dev",
    "dev:docker": "docker compose -f ./config/docker-compose.yml down -v && docker compose -f ./config/docker-compose.yml up -d db",
    "dev:docker:stop": "docker compose -f ./config/docker-compose.yml down -v",
    "dev:migrate": "prisma migrate dev",
    "dev:setup": "sh ./scripts/dev/setup.sh",
    "dev:setup:vscode": "yarn dlx @yarnpkg/sdks vscode",
    "start": "yarn next start",
    "test": "yarn test:lint && yarn test:type",
    "test:lint": "eslint --ext cjs,js,jsx,ts,tsx .",
    "test:type": "tsc -p ./tsconfig.dev.json"
  },
  "dependencies": {
    "@fontsource/azeret-mono": "4.5.12",
    "@fontsource/poppins": "4.5.10",
    "@ivangabriele/tsconfig-next": "1.2.2",
    "@prisma/client": "4.9.0",
    "@swc/core": "1.3.27",
    "@types/chess.js": "0.13.4",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.18",
    "@types/ramda": "0.28.21",
    "@types/react": "18.0.27",
    "@types/styled-components": "5.1.15",
    "@yarnpkg/pnpify": "4.0.0-rc.36",
    "bhala": "3.0.4",
    "bootstrap": "5.2.3",
    "chess.js": "0.13.3",
    "chessground": "8.3.6",
    "csvtojson": "2.0.10",
    "dotenv": "16.0.3",
    "esm-path": "1.0.1",
    "ky": "0.33.2",
    "ky-universal": "0.11.0",
    "lodash.debounce": "4.0.8",
    "next": "13.1.4",
    "numeral": "2.0.6",
    "ora": "6.1.2",
    "p-queue": "7.3.0",
    "prisma": "4.9.0",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-bootstrap": "2.4.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-spinners": "0.13.8",
    "styled-components": "5.3.3",
    "type-fest": "3.5.3",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-typescript-next": "6.0.1",
    "@ivangabriele/prettier-config": "3.1.0",
    "@types/numeral": "2.0.2",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3"
  },
  "lint-staged": {
    "*.{cjs,js,jsx,ts,tsx}": "eslint --ext cjs,js,jsx,ts,tsx"
  },
  "packageManager": "yarn@3.3.1",
  "prettier": "@ivangabriele/prettier-config"
}
